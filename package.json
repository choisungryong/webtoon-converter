{
    "name": "webtoon-converter",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build && npx @opennextjs/cloudflare build && node -e \"const fs=require('fs'); const src='.open-next/assets'; const srcW='.open-next/worker.js'; const dst='.vercel/output/static'; if(fs.existsSync(src)){fs.mkdirSync(dst,{recursive:true});fs.cpSync(src,dst,{recursive:true}); if(fs.existsSync(srcW))fs.copyFileSync(srcW,dst+'/_worker.js'); console.log('Fixed output path');}\"",
        "preview": "npx @opennextjs/cloudflare build && npx @opennextjs/cloudflare preview",
        "deploy": "npx @opennextjs/cloudflare build && npx @opennextjs/cloudflare deploy",
        "db:init:prod": "wrangler d1 execute webtoon-db --file=./schema.sql --yes",
        "start": "next start",
        "lint": "next lint",
        "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
    },
    "dependencies": {
        "@ant-design/icons": "^6.1.0",
        "@ant-design/nextjs-registry": "^1.3.0",
        "@opennextjs/cloudflare": "latest",
        "@types/uuid": "^10.0.0",
        "antd": "5.12.0",
        "lucide-react": "0.294.0",
        "next": "15.1.0",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "uuid": "^13.0.0"
    },
    "devDependencies": {
        "@types/node": "20.10.4",
        "@types/react": "18.2.45",
        "@types/react-dom": "18.2.18",
        "autoprefixer": "10.4.16",
        "esbuild": "^0.24.0",
        "postcss": "8.4.32",
        "tailwindcss": "3.4.0",
        "typescript": "5.3.3",
        "wrangler": "^3.99.0"
    }
}